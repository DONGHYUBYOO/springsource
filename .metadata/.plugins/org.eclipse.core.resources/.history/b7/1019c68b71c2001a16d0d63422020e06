<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springmapper.PartnerMapper">
	
	<insert id="registPartner">
		insert into partner(id, name, ceo, contact, address) values(partner_seq.nextval, #{name}, #{ceo}, #{contact}, #{address})
	</insert>
	
	
	<select id="listPartner" resultType="com.spring.domain.PartnerVO">
		select * from partner
	</select>
</mapper>